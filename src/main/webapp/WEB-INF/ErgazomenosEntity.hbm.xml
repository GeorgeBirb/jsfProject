<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.models.ErgazomenosEntity" table="ergazomenos" schema="etaireia">
        <property name="onoma">
            <column name="ONOMA" sql-type="text" not-null="true"/>
        </property>
        <property name="arxPatr">
            <column name="ARX_PATR" sql-type="text" not-null="true"/>
        </property>
        <property name="epitheto">
            <column name="EPITHETO" sql-type="text" not-null="true"/>
        </property>
        <property name="arTayt">
            <column name="AR_TAYT" sql-type="int" not-null="true"/>
        </property>
        <property name="hmGen">
            <column name="HM_GEN" sql-type="text" not-null="true"/>
        </property>
        <property name="dieythynsi">
            <column name="DIEYTHYNSI" sql-type="text" not-null="true"/>
        </property>
        <property name="fylo">
            <column name="FYLO" sql-type="text" not-null="true"/>
        </property>
        <property name="misthos">
            <column name="MISTHOS" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="proistamenos">
            <column name="PROISTAMENOS" sql-type="int" not-null="true"/>
        </property>
        <property name="arithT">
            <column name="ARITH_T" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>